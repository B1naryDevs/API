import javafx.event.ActionEvent;
import javafx.fxml.FXML;
import javafx.scene.control.Button;
import javafx.scene.control.TextField;

public class candidatoControler {

    @FXML
    private Button botaoAvancar;

    @FXML
    private TextField cpfCandidato;
    int cpf;
    
    public void submit(ActionEvent event){
        
        try {
            cpf = Integer.parseInt(cpfCandidato.getText());
            System.out.println(cpf);   
        }
        catch (NumberFormatException e){
            System.out.println("Insira apenas n√∫meros por favor");
        }
        catch (Exception e) {
            System.out.println(e);
        }
    }


    @FXML
    private TextField dataCandidato;

    @FXML
    private TextField emailCandidato;

    @FXML
    private TextField nomeCandidato;

    @FXML
    private TextField senhaCandidato;

    @FXML
    private TextField telefoneCandidato;

    @FXML
    public void cadastrarDados(ActionEvent event) {

    }
}
